{
  "name": "Run command prefixed by `03 using Win+R and UAC bypass with PowerShell",

  "setup": {
    "options": [],
    "adf": [
      {
        "criteria": [
          ["stringatstart", "`03"]
        ],
        "actions": [
          ["pauseduration", 1.0],
          ["sendgui", "R"], // Win + R
          ["sendpause"],
          ["sendtext" "powershell start-process -verb runas "], // PowerShell command to run as admin
          ["skipcharacters", 3], // Skips `03 in buffer
          ["sendremaining"], // Adds command in barcode
          ["sendenter"],
          ["pauseduration", 2.0],
          ["sendpause"],
          ["sendalt", "Y"], // Presses Yes on UAC dialog
          ["sendpause"],
          ["sendenter"] // If we already got to a command prompt (no UAC), this clears the "Y" from the prompt
        ]
      }
    ]
  }
}