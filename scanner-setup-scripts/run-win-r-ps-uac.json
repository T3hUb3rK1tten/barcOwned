{
  "name": "Run command w/ UAC Bypass",
  "description": "Run command prefixed by `03 using Win+R with UAC bypass via PowerShell",

  "setup": {
    "options": [],
    "rules": [
      {
        "criteria": [
          ["stringatstart", "`03"]
        ],
        "actions": [
          ["pauseduration", 1.0],
          ["sendgui", "R"],
          ["sendpause"],
          ["sendtext", "powershell start-process -verb runas "],
          ["skipcharacters", 3],
          ["sendremaining"],
          ["sendenter"],
          ["pauseduration", 2.0],
          ["sendpause"],
          ["sendalt", "Y"],
          ["sendpause"],
          ["sendenter"]
        ]
      }
    ]
  }
}
